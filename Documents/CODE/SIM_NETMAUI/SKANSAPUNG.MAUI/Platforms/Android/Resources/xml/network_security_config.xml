<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="false">
        <!-- Ganti 'your.api.domain' dengan domain API Anda -->
        <domain includeSubdomains="true">your.api.domain</domain>
        <pin-set>
            <!-- GANTI DENGAN HASH KUNCI PUBLIK SERTIFIKAT ANDA (SHA-256) -->
            <pin digest="SHA-256">GANTI_DENGAN_HASH_SERTIFIKAT_ANDA</pin>
            <!-- Anda dapat menambahkan pin cadangan di sini -->
            <!-- <pin digest="SHA-256">GANTI_DENGAN_HASH_SERTIFIKAT_CADANGAN</pin> -->
        </pin-set>
    </domain-config>
</network-security-config>
